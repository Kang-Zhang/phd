	Journal = {International Journal of Pressure Vessels and Piping},
	Key = {Pressure vessels},
	Keywords = {Cylinders (containers);Fatigue of materials;Crack propagation;Mathematical models;Finite element method;Stress intensity factors;Degrees of freedom (mechanics);Aspect ratio;},
	Language = {English},
	Note = {Pressurized cylinders;},
	Number = {3},
	Pages = {293 - 300},
	Title = {Numerical analysis of fatigue growth of external surface cracks in pressurized cylinders},
