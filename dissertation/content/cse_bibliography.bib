@article{ravichandran1997,
	Abstract = {A simple simulation of alternate growth of a small surface crack in
	the surface and depth directions was performed to illustrate the
	changes in crack aspect ratio, induced by grain boundaries, as a
	function of crack size. It is shown that at small crack sizes, large
	variations in aspect ratio, $a/c$ ($a$ is the crack depth and $c$
	is the half-surface length), occur, due to local crack front perturbations
	induced by grains that are oriented for crack growth. At these crack
	sizes, the assumption of a semicircular crack shape ($a/c = 1.0$)
	was found to cause errors in stress intensity range ($\Delta K$)
	calculations. This, in turn, led to significant scatter or 'anomaly'
	in small crack growth rates relative to large cracks. At large crack
	sizes, the effects of local crack front perturbations on crack aspect
	ratio and $\Delta K$ were found to be insignificant. As a result,
	the scatter in crack growth data was found to decrease to a negligible
	level at large crack sizes. It is suggested that the limiting crack
	size above which the small crack behaves as a large crack, $l_2 =
	10d$ ($d$ = grain size), proposed by Taylor and Knott, is related
	to the crack size above which the effects due to aspect ratio variations
	are small.},
	Author = {K.S. Ravichandran},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	File = {:\\\\files.cae.tntech.edu\\homes\\Documents\\Projects\\TTU\\renfro - PhD Research\\CSE Papers\\PDF\\effects of crack aspect ratio on the behavior of small surface cracks in fatigue - part 1.pdf:PDF},
	Issn = {1073-5623},
	Journal = {Metallurgical and Materials Transactions A: Physical Metallurgy and Materials Science},
	Key = {Crack propagation},
	Keywords = {Simulation;Grain boundaries;Stress intensity factors;Fatigue of materials;Metallographic microstructure;},
	Language = {English},
	Note = {Surface crack;Stress intensity range;Crack size;},
	Number = {1},
	Pages = {149 - 156},
	Title = {Effects of crack aspect ratio on the behavior of small surface cracks in fatigue: Part I. Simulation},
	Volume = {28A},
	Year = {1997}}

@article{ravichandran1997a,
	Abstract = {A basic study is performed on the evolution of three-dimensional shapes
	of small surface fatigue cracks during fatigue, and the effect of
	this evolution on small-crack growth behavior of a titanium-base
	alloy. Specifically, the nature and the magnitude of variations in
	crack aspect ratio, $a/c$ ($a$ is the crack depth and $c$ is the
	half-surface crack length), during cyclic crack growth and its impact
	on growth rates are studied. Experiments are performed on naturally
	initiated micro-cracks in a microstructure consisting of equiaxed
	primary-$\alpha_{vv}2$ FIXME phase in a Widmanstatten (transformed
	$\beta$) matrix. Several cracks under stress ratio ($R$) levels of
	0.1 and -1, are studied. Apparent aspect ratios of surface cracks
	are calculated from the compliance response and the surface crack
	length data as a function of fatigue cycles. Measurements of closure
	levels of small cracks are also performed and are used to partly
	account for the differences in growth rates.},
	Author = {K.S. Ravichandran},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	File = {three-dimensional crack-shape effects during the growth of small surface fatigue cracks in a titanium-base alloy.pdf:three-dimensional crack-shape effects during the growth of small surface fatigue cracks in a titanium-base alloy.pdf:PDF},
	Issn = {8756-758X},
	Journal = {Fatigue and Fracture of Engineering Materials \& Structures},
	Key = {Crack propagation},
	Keywords = {Titanium alloys;Fatigue of materials;Metallographic microstructure;Microcracks;Crack initiation;Aspect ratio;Stresses;Interferometry;Microscopic examination;},
	Language = {English},
	Note = {Fatigue crack closure;Stress ratio;Surface crack length;},
	Number = {10},
	Pages = {1423 - 1442},
	Title = {Three-dimensional crack-shape effects during the growth of small surface fatigue cracks in a titanium-base alloy},
