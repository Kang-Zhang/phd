@article{wilson1995,
	Abstract = {The verification and validation (V \& V) of a fracture mechanics life
	prediction computer code is an essential task. The confidence level
	of results is directly related to the effort invested in the V \&
	V process. The V \& V process of life prediction software is broken
	two major parts: computer code assessment and fracture mechanics
	assessment. The fracture mechanics assessment is divided into three
	V \& V tasks: fracture mechanics parameters, baseline predictions,
	and advanced predictions. The details of validating the stress intensity
	factor solution for a surface crack in a finite plate is discussed.
	Example V \& V results are reported for NASCRAC Version 2.0 and NASA/FLAGRO
	Version 1.3. NASCRAC has significant errors in implementing a fatigue
	crack growth algorithm. In addition, the surface crack stress intensity
	factor solution in NASCRAC does not agree well with established analytical
	and experimental benchmark results. NASA/FLAGRO has computer coding
	problems which affect its portability.},
	Address = {Idaho Falls, ID, USA},
	Author = {Christopher~D. Wilson},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	Date-Modified = {2013-04-10 19:04:14 +0000},
	Issn = {0066-0558},
	Journal = {ASTM Special Technical Publication 1256},
	Key = {Fracture mechanics},
	Keywords = {Computer software;Crack propagation;Forecasting;Cracks;Stress intensity factors;Errors;Algorithms;Encoding (symbols);},
	Language = {English},
	Note = {Fracture mechanics life prediction code;Software verification;Software validation;Fatigue crack growth;Surface cracks;Crack shape evolution;Computer code assessment;Fracture mechanics assessment;},
	Pages = {779--798},
	Title = {Fracture mechanics life prediction computer code verification and validation},
	Year = {1995}}

@article{wu2006,
	Abstract = {A new method is developed for the evaluation of a crack shape based
	on a given stress intensity factor (SIF) distribution for a surface
	crack under Mode-I loading conditions. The SIF distribution along
	the crack front is investigated using a direct simulation technique,
	in which the effect of crack closure on fatigue crack growth is considered.
	Then a SIF distribution function is chosen based on the numerical
	results. Crack shape (and the SIF) is achieved based on the given
	SIF distribution function using a numerical iterative procedure.
	Empirical SIF equations for surface cracks in plates subjected to
	tension and pure bending fatigue load are determined by systematic
	curve fitting of the numerical results. The depth ratio and the aspect
	ratio are considered in the ranges of 0.1-0.9 and 0.2-1.2, respectively.
	The aspect-ratio variation of surface cracks under fatigue loading
	is predicted. The application of the new method to predict the shape
	of a surface crack in plates subjected to tension and pure bending
	and comparisons of the results obtained with the predictions of the
	empirical equations proposed by Newman and Raju are presented. \copyright
	2006 Elsevier Ltd. All rights reserved.},
	Author = {Zhixue Wu},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	File = {:the shape of a surface crack in a plate based on a given stress intensity factor distribution.pdf:PDF},
	Issn = {0308-0161},
	Journal = {International Journal of Pressure Vessels and Piping},
	Key = {Crack initiation},
	Keywords = {Crack propagation;Stress intensity factors;Finite element method;Iterative methods;Plates (structural components);Fatigue testing;Curve fitting;Aspect ratio;},
	Language = {English},
	Note = {Crack shape;Surface crack;Structural assessment;},
	Number = {3},
	Pages = {168 - 180},
	Title = {The shape of a surface crack in a plate based on a given stress intensity factor distribution},
	Url = {http://dx.doi.org/10.1016/j.ijpvp.2006.01.004},
	Volume = {83},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.ijpvp.2006.01.004}}

@article{yeeburnslambertlecsekmohaupt1995,
	Abstract = {The effect of plate width on the initiation and propagation of fatigue
	cracks in plate-to-plate T-joints with load-carrying transverse attachment
	plates and flat fillet-like weld profiles was investigated in a series
	of constant amplitude fatigue tests. There was no observable effect
	of plate width on initiation life, propagation life, or total fatigue
	life, but plate width had a significant effect on crack shape development
	and crack growth rates. More cracks initiated along the weld toes
	of wider joints. As a result, the aspect ratios of dominant surface
	cracks were lower in wider joints, and the dominant surface cracks
	propagated faster through the thickness of wider base plates. However,
	there was a greater propensity for edge cracking in narrower specimens
	because fatigue cracks initiated closer to the free edges of such
	joints. This offset the faster growth of dominant surface cracks
	in wider joints so that there was no net effect of plate width on
	propagation life. A multiple crack linear elastic fracture mechanics
	model successfully simulated these differences in crack shape development
	behaviour.},
	Address = {Copenhagen, Den},
	Author = {R. Yee and D.J. Burns and S.B. Lambert and R.L. Lecsek and U.H. Mohaupt},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	Journal = {Proceedings of the International Conference on Offshore Mechanics and Arctic Engineering - OMAE},
	Key = {Welds},
	Keywords = {Fatigue of materials;Cracks;Crack propagation;Coalescence;Plates (structural components);Crack initiation;Fatigue testing;Service life;Aspect ratio;},
	Language = {English},
	Note = {Initiation life;Propagation life;Crack shape development;},
	Pages = {329 - 335},
	Title = {Effect of plate width on the growth and coalescence of fatigue cracks in plate-to-plate welded T-joints},
	Volume = {3},
	Year = {1995}}

