@article{rhee1987,
	Abstract = {A fracture mechanics approach to fatigue crack growth analysis for
	offshore structural tubular joints is described. Various aspects
	of a fatigue crack growth analysis ranging from the stress intensity
	factor solutions to a simulation of fatigue crack coalescence process
	of tubular joint weld toe surface flaws are also discussed. The fully
	mixed-mode stress intensity factors of nine weld toe surface cracks
	of an X-joint under brace tension loading were calculated by detailed
	three-dimensional finite element analyses. Using these stress intensity
	factor solutions, a fatigue crack growth study was performed for
	the X-joint until the crack surface length grew to two times the
	chord thickness. Through this study, the crack shape change during
	the fatigue crack propagation was investigated in detail. The result
	of the study showed that the aspect ratios between the crack surface
	lengths and depths of propagating cracks converged to about six to
	one, regardless of their initial shapes and depths. Using this observation
	and other stress analysis results, an engineering model was developed
	for fatigue crack growth analyses of surface flaws in the X-joint.},
	Address = {Houston, TX, USA},
	Author = {C. Rhee},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	Journal = {Proceedings of the International Offshore Mechanics and Arctic Engineering Symposium},
	Key = {OFFSHORE STRUCTURES -- Structural Analysis},
	Keywords = {JOINTS - Fatigue;STEEL - Crack Propagation;STEEL STRUCTURES - Tubular;FRACTURE MECHANICS;WELDS - Defects;},
	Language = {English},
	Note = {FATIGUE CRACK GROWTH;TUBULAR JOINTS;STRESS INTENSITY FACTOR;WELD TOE;},
	Pages = {339 - 347},
	Title = {FATIGUE CRACK GROWTH ANALYSES OF OFFSHORE STRUCTURAL TUBULAR JOINTS.},
	Volume = {3},
	Year = {1987}}

