@article{linsmith1999a,
	Abstract = {A multiple degree of freedom numerical procedure applicable to the
	prediction of the fatigue crack growth of surface cracks in plates
	under a combined tension and bending load is described. This procedure
	performs a three-dimensional finite element analysis to estimate
	the stress intensity factors at a set of points along the crack front,
	and then calculates the crack growth increments at these points invoking
	a fatigue crack growth relationship. A new crack front is established
	using a cubic spline approximation. A remeshing technique developed
	enables the procedure to be implemented automatically, and then fatigue
	crack growth can, therefore, be predicted in a step-by-step way.
	Following the descriptions of some theoretical background and technical
	details, several important problems in the evaluation of stress intensity
	factors by the finite element method are discussed. They include
	the sensitivity of stress intensity factor results to crack shape,
	the effect of mesh orthogonality and the $J$-integral path independence.
	It is shown that the stress intensity factor results are sensitive
	to the crack front shape, that the cubic spline approximation to
	a crack front can eliminate the oscillation of stress intensity factors
	around the crack front defined by the polygonal line and consequently
	increase the stress intensity factor accuracy, and that the $J$-integral
	path independence is usually maintained but is lost at the free surface
	if a slightly non-orthogonal intersection exists between the free
	surface and the crack front.},
	Author = {X.B. Lin and R.A. Smith},
	Copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
	File = {:\\\\files.cae.tntech.edu\\homes\\Documents\\Projects\\TTU\\renfro - PhD Research\\CSE Papers\\PDF\\finite element modelling of fatigue crack growth of surface cracked plates - part 3.pdf:PDF;:\\\\files.cae.tntech.edu\\homes\\Documents\\Projects\\TTU\\renfro - PhD Research\\CSE Papers\\PDF\\finite element modelling of fatigue crack growth of surface cracked plates - part 2.pdf:PDF;:\\\\files.cae.tntech.edu\\homes\\Documents\\Projects\\TTU\\renfro - PhD Research\\CSE Papers\\PDF\\finite element modelling of fatigue crack growth of surface cracked plates - part 1.pdf:PDF},
	Issn = {0013-7944},
	Journal = {Engineering Fracture Mechanics},
	Key = {Plate metal},
	Keywords = {Cracks;Fatigue of materials;Crack propagation;Finite element method;Stress intensity factors;Degrees of freedom (mechanics);Surfaces;Tensile stress;Bending (deformation);Approximation theory;},
	Language = {English},
	Note = {Surface cracks;Three dimensional finite element analysis;Crack front;Cubic spline approximation;Crack shape;Mesh orthogonality;J-integral path independence;},
	Number = {5},
	Pages = {503 - 522},
	Title = {Finite element modelling of fatigue crack growth of surface cracked plates, Part I: the numerical technique},
	Url = {http://dx.doi.org/10.1016/S0013-7944(99)00040-5},
	Volume = {63},
