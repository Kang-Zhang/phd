@article{rajunewman1979a,
	__Markedentry = {[renfro:1]},
	Abstract = {The purpose of this paper is to present stress-intensity factors,
	calculated by a three-dimensional finite-element analysis, for shallow
	or deep quarter-elliptical corner cracks at the edge of a hole in
	a finite-thickness plate. The plate was subjected to remote uniform
	tension, remote bending, or simulated pin loading in the hole. A
	wide range of configuration parameters was investigated. The crack
	depth-to-plate thickness ranged from 0.2 to 0.8, while the ratio
	of crack depth to crack length ranged from 0.2 to 2. The ratio of
	hole radius to plate thickness was 0.5 or 1. To verify the accuracy
	of the three-dimensional finite-element models employed, convergence
	was studied by varying the numbers of degrees of freedom (the number
	range from 4400 to 9300). The stress-intensity factor variations
