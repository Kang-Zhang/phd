	Issn = {0142-1123},
	Journal = {International Journal of Fatigue},
	Key = {Fatigue testing},
	Keywords = {Crack initiation;Surface treatment;Crack propagation;Fatigue of materials;Aspect ratio;Finite element method;Computer software;},
	Language = {English},
	Note = {Fatigue crack closure;Semi-circular cracks;Semi-elliptical cracks;Surface cracks;Crack shape development;},
	Number = {11},
	Pages = {1225--1239},
	Title = {Three-dimensional finite element analysis of fatigue crack closure behavior in surface flaws},
	Url = {http://dx.doi.org/10.1016/j.ijfatigue.2004.02.007},
	Volume = {26},
	Year = {2004},
