	Key = {Surface properties},
	Keywords = {Cracks;Magnetic field measurement;Wavelet transforms;Sensors;Nondestructive examination;Fatigue of materials;Profilometry;Multilayer neural networks;Approximation theory;Random processes;Gradient methods;Algorithms;},
	Language = {English},
	Note = {Surface magnetic field measurement (SMFM);NDE;Wavelet networks;},
	Pages = {2251 - 2254},
	Title = {A Wavelet Network Approach for predicting Surface Cracks Shapes},
	Url = {http://dx.doi.org/10.1109/IECON.2003.1280594},
