@article{corn1971,
	Abstract = {A study has been made of two different cracking techniques for obtaining
	partial-thickness cracks. These are the axial fatigue and the flexural
	fatigue methods. Both methods were found to yield a preferred propagation
	path (PPP) in the crack depth-crack half-length plane. The PPP is
	defined as the propagation path which the crack follows after the
	starter notch no longer influences it, and was markedly different
	for the two cracking techniques. The axial PPP was found to be a
	straight line with $a/c$ approximately equal to 0.9, where $a$ is
	crack depth and $c$ is crack half-length. The flexural PPP was found
	to be a curve that initially had a slope of $a/c = 1$, but as the
	crack depth increased, the change in crack depth with respect to
	crack length decreased and the curve appeared to asymptotically approach
	a line between one-half and five-eighths of the thickness. Whereas
	the flexural fatigue cracks showed little tendency to exceed half-thickness,
	the axial fatigue cracks in this study penetrated easily up to 92
	per cent of the thickness. The reason for this different behavior
	is suggested to be the stress gradient associated with bending which
	is absent for the axial fatigue case.},
	Addendum = {\\ \includegraphics[width=0.75\textwidth]{Corn_Paper/ac_plot} \includegraphics[width=0.75\textwidth]{Corn_Paper/GrowthSequence}},
	Author = {Corn, D.L.},
	Copyright = {Compilation and indexing terms, Copyright 2008 Elsevier Inc.},
	Date-Modified = {2012-12-10 16:36:24 +0000},
	File = {a study of cracking techniques for obtaining partial thickness cracks of pre-selected depths and shapes.pdf:a study of cracking techniques for obtaining partial thickness cracks of pre-selected depths and shapes.pdf:PDF},
	Journal = {Engineering Fracture Mechanics},
	Language = {English},
	Number = {1},
	Pages = {45--52},
	Title = {A study of cracking techniques for obtaining partial thickness cracks of pre-selected depths and shapes},
	Volume = {3},
	Year = {1971}}

@article{cruse1988,
	Abstract = {This paper discusses a variety of issues associated with the elastic
	fracture mechanics modeling of surface cracks; both analytical and
	experimental issues are addressed. The primary analytical focus is
	on the use of the boundary integral equation method for elastic fracture
	mechanics analysis. An advanced code is highlighted. A new formulation
	for three-dimensional crack-surface integral equations is presented
	with some new analytical results associated with the free-surface
	singularity. Essential agreements with the results of Benthem are
	obtained. Fatigue crack growth results for surface cracks in tension
	and bending are compared with analytical solutions. It is shown that
	surface cracks in tension grow essentially in accord with an elastic
	fracture mechanics prediction, whereas surface cracks in bending
	do not conform to the elastic prediction, based on surface values
	of the elastic stress-intensity factor. Some thoughts for further
	research are given.},
	Addendum = {\\ \includegraphics[width=0.75\textwidth]{Cruse_3dElasticSurfaceCracks/CrackShapes}},
	Address = {San Diego, CA, USA},
	Author = {T.A. Cruse},
	Copyright = {Compilation and indexing terms, Copyright 2008 Elsevier Inc.},
	Journal = {ASTM Special Technical Publication},
	Keywords = {analytical methods, computational fracture mechanics methods, elastic fracture mechanics, fatigue, life prediction, stress analysis, surface cracks. fracture mechanics},
	Pages = {19--42},
	Title = {Three-Dimensional Elastic Surface Cracks},
	Url = {http://www.worldcat.org/title/fracture-mechanics-nineteenth-symposium-nineteenth-national-symposium-on-fracture-mechanics/oclc/122328648},
